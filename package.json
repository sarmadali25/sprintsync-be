{
  "name": "sprintsync-backend",
  "version": "1.0.0",
  "description": "Backend for task assignment",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "watch": "nodemon --exec ts-node src/server.ts",
    "migrate": "NODE_ENV=development npx sequelize-cli db:migrate",
    "migrate:undo": "NODE_ENV=development npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
    "migrate:status": "NODE_ENV=development npx sequelize-cli db:migrate:status",
    "migrate:production": "NODE_ENV=production npx sequelize-cli db:migrate",
    "migrate:status:production": "NODE_ENV=production npx sequelize-cli db:migrate:status",
    "db:create": "NODE_ENV=development npx sequelize-cli db:create",
    "db:create:production": "NODE_ENV=production npx sequelize-cli db:create",
    "db:drop": "NODE_ENV=development npx sequelize-cli db:drop",
    "db:drop:production": "NODE_ENV=production npx sequelize-cli db:drop"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/sequelize": "^6.12.0",
    "nodemon": "^3.0.0",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "author": "",
  "license": "ISC"
}
